generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Player {
  id Int @id @default(autoincrement())
  firstName String
  lastName String
  position String
  team String
}

model Game {
  id Int @id @default(autoincrement())
  date DateTime
  homeTeam String
  awayTeam String
  homeScore Int
  awayScore Int
}
